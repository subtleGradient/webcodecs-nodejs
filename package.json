  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/browser": "^4.0.14",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "playwright": "^1.57.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}

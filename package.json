{
  "name": "webcodecs-nodejs",
  "version": "0.1.0",
  "description": "WebCodecs API implementation for Node.js using N-API + FFmpeg",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "gypfile": true,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "src/native/",
    "binding.gyp"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:browser": "vitest run -c vitest.browser.config.ts",
    "test:node": "vitest run",
    "build": "tsc",
    "build:native": "node-gyp rebuild",
    "rebuild": "node-gyp rebuild",
    "clean:native": "node-gyp clean",
    "typecheck": "tsc --noEmit",
    "generate:fixtures": "npx tsx fixtures/generate.ts",
    "demo": "npx tsx demo.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "webcodecs",
    "video",
    "audio",
    "encoder",
    "decoder",
    "media",
    "ffmpeg",
    "napi",
    "vp8",
    "vp9",
    "h264",
    "codec"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subtleGradient/webcodecs-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/subtleGradient/webcodecs-nodejs/issues"
  },
  "homepage": "https://github.com/subtleGradient/webcodecs-nodejs#readme",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/qrcode": "^1.5.6",
    "@vitest/browser": "^4.0.14",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "node-addon-api": "^8.3.1",
    "node-gyp": "^11.2.0",
    "playwright": "^1.57.0",
    "tsx": "^4.19.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "jsqr": "^1.4.0",
    "qrcode": "^1.5.4"
  }
}

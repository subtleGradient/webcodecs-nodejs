{
  "name": "webcodecs-nodejs",
  "version": "1.0.0",
  "description": "WebCodecs API implementation for Node.js",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:browser": "vitest run -c vitest.browser.config.ts",
    "test:node": "vitest run",
    "build": "tsc",
